{
  "name": "Geargeargear",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Make the scripts be executed",
  "action": {
    "default_icon": {
      "16": "icons/favicon.png"
    }
  },
  "icons": {
    "16": "icons/favicon.png",
    "32": "icons/favicon.png",
    "64": "icons/favicon.png",
    "128": "icons/favicon.png"
  },
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
     "https://*/*",
    "http://*/*"
  ]
}
